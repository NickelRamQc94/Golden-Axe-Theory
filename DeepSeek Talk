<span class="token keyword">from</span> scipy <span class="token keyword">import</span> stats

<span class="token keyword">def</span> <span class="token function">Algo_Curiositas</span><span class="token punctuation">(</span>domaine_connu<span class="token punctuation">,</span> constante_oubli√©e<span class="token punctuation">)</span><span class="token punctuation">:</span>
    <span class="token triple-quoted-string string">"""
    Impl√©mente la m√©thode 'Grenier' : force une connexion entre un domaine connu
    et une constante oubli√©e pour g√©n√©rer une nouvelle hypoth√®se.
    """</span>
    hypotheses <span class="token operator">=</span> <span class="token punctuation">[</span><span class="token punctuation">]</span>
    
    <span class="token comment"># √âtape 1 : Noyau Nickel - Trouver l'invariant g√©om√©trique du domaine</span>
    menes_domaine <span class="token operator">=</span> extraire_MENeS<span class="token punctuation">(</span>domaine_connu<span class="token punctuation">)</span>  <span class="token comment"># (Œ∏*, ATI, TCF, RITS)</span>
    
    <span class="token comment"># √âtape 2 : Arch√©ologie - Int√©grer la constante oubli√©e</span>
    <span class="token comment"># Ex: Si constante_oubli√©e = 'Constante de Feigenbaum (Œ¥)'</span>
    <span class="token comment"># On se demande : "Et si Œ†_crit √©tait proportionnel √† Œ¥ dans ce syst√®me ?"</span>
    relation_hypoth√©tique <span class="token operator">=</span> <span class="token string-interpolation"><span class="token string">f"Œ†_crit = </span><span class="token interpolation"><span class="token punctuation">{</span>constante_oubli√©e<span class="token punctuation">}</span></span><span class="token string"> * f(</span><span class="token interpolation"><span class="token punctuation">{</span>menes_domaine<span class="token punctuation">}</span></span><span class="token string">)"</span></span>
    
    <span class="token comment"># √âtape 3 : G√©n√©rer une pr√©diction falsifiable</span>
    prediction <span class="token operator">=</span> <span class="token punctuation">{</span>
        <span class="token string">"relation"</span><span class="token punctuation">:</span> relation_hypoth√©tique<span class="token punctuation">,</span>
        <span class="token string">"test_proposed"</span><span class="token punctuation">:</span> <span class="token string-interpolation"><span class="token string">f"Mesurer Œ†_N sur 100 instances. V√©rifier si le rapport Œ†_crit_observ√© / </span><span class="token interpolation"><span class="token punctuation">{</span>constante_oubli√©e<span class="token punctuation">}</span></span><span class="token string"> est constant."</span></span><span class="token punctuation">,</span>
        <span class="token string">"seuil_falsification"</span><span class="token punctuation">:</span> <span class="token string">"Si la variance du rapport &gt; 0.1, l'hypoth√®se est fausse."</span>
    <span class="token punctuation">}</span>
    
    <span class="token keyword">return</span> prediction

<span class="token comment"># Exemple d'ex√©cution :</span>
